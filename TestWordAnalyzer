import java.io.File;
import java.io.IOException;

public class TestWordAnalyzer {
    public static void main(String[] args) throws IOException {
        String[] keywords={"else","if","then","while","do"};
        char[] op={'+','-','*','/','>','<','='};
        char[] divide={'(',')',';'};
        File file=new File("/usr/TH/wordanalyzer.txt");
        new WordAnalyzer().wordAnalyze(keywords,op,divide,file);
    }
}
